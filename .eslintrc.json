{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "@nuxtjs",
    "airbnb-base",
    "prettier",
    "plugin:vue/recommended",
    "plugin:nuxt/recommended",
    "plugin:vue/essential"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "parser": "babel-eslint"
  },
  "plugins": [
    "vue",
    "prettier"
  ],
  "rules": {}
}
